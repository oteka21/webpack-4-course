{
  "name": "webpack-4",
  "version": "1.0.0",
  "description": "Curso de webpack 4 de platzi ",
  "main": "index.js",
  "scripts": {
    "build": "webpack --entry ./index.js --output ./bundle.js --mode development",
    "build:local": "webpack --mode production",
    "build:external": "webpack --config ./external/webpack.config.js",
    "build:multiple": "webpack --config ./multiple-entry-points/webpack.config.js",
    "build:css": "webpack --config ./css-style-loader/webpack.config.js",
    "build:plugins": "webpack --config ./plugins/webpack.config.js",
    "build:dev": "webpack-dev-server --config ./dev-server/webpack.config.js",
    "build:babel": "webpack-dev-server --config ./babel/webpack.config.js",
    "build:react": "webpack-dev-server --config ./react/webpack.config.js"
  },
  "author": "Leonardo oteca",
  "license": "MIT",
  "dependencies": {
    "@babel/runtime": "7.8.0",
    "react": "16.12.0",
    "react-dom": "16.12.0"
  },
  "devDependencies": {
    "@babel/core": "7.8.0",
    "@babel/plugin-transform-runtime": "7.8.0",
    "@babel/preset-env": "7.8.0",
    "@babel/preset-react": "7.8.0",
    "babel-loader": "8.0.6",
    "css-loader": "2.1.1",
    "html-webpack-plugin": "3.2.0",
    "mini-css-extract-plugin": "0.9.0",
    "style-loader": "1.1.2",
    "webpack": "4.32.2",
    "webpack-cli": "3.3.2",
    "webpack-dev-server": "3.10.1"
  }
}
